{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
      "moduleResolution": "nodenext",
      "module": "nodenext", 
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}
